{
  "name": "creasense-vue3",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build": "npm run generate-locales && npm run generate-protocols && run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "generate-locales": "node scripts/generate-locales.js",
    "generate-protocols": "node scripts/generate-protocols.js",
    "cap:sync": "npx cap sync",
    "cap:sync:ios": "npx cap sync ios",
    "cap:build:ios": "npm run build && npm run cap:sync:ios && npx cap open ios"
  },
  "dependencies": {
    "@capacitor-community/bluetooth-le": "7.1.1",
    "@capacitor/app": "7.0.1",
    "@capacitor/camera": "7.0.1",
    "@capacitor/core": "7.4.2",
    "@capacitor/device": "7.0.1",
    "@capacitor/ios": "7.4.2",
    "@capacitor/splash-screen": "7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@capgo/capacitor-native-biometric": "7.1.13",
    "@ionic/vue": "8.6.5",
    "@ionic/vue-router": "8.6.5",
    "capacitor-native-settings": "7.0.2",
    "chart.js": "4.5.0",
    "pinia": "3.0.3",
    "vue": "3.5.18",
    "vue-chartjs": "5.3.2",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "22.0.2",
    "@types/node": "22.17.0",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.32.0",
    "eslint-plugin-vue": "10.3.0",
    "jiti": "2.5.1",
    "npm-run-all2": "8.0.4",
    "typescript": "5.8.3",
    "vite": "7.0.6",
    "vite-plugin-vue-devtools": "8.0.0",
    "vue-tsc": "3.0.4"
  }
}
